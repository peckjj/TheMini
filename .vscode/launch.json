  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "skipFiles": ["<node_internals>/**"],
      "program": ".\\dist\\index.js",
      "outFiles": ["./dist/**/*.js"],
      "preLaunchTask": "tsc: build - themini-backend/tsconfig.json",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/themini-backend"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Charset Builder",
      "skipFiles": ["<node_internals>/**"],
      "program": ".\\buildCharSetBank.js",
      "cwd": "${workspaceFolder}/themini-backend/sql"
    }
  ]
}
